    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^7.4.4",
    "mongoose": "^6.0.11",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
